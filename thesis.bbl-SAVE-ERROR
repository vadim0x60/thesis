      \strng{namehash}{89858204bf3d4abc5e7cd773801b3a7b}
      \strng{fullhash}{1de99bc0ea981f1055941279b1aa03ac}
      \strng{bibnamehash}{89858204bf3d4abc5e7cd773801b3a7b}
      \strng{authorbibnamehash}{89858204bf3d4abc5e7cd773801b3a7b}
