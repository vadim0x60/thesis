      \strng{namehash}{29c20c203ca1398dbc23ba83e1795904}
      \strng{fullhash}{6b870d58c61f26c1088635ee4f7eb96b}
      \strng{bibnamehash}{29c20c203ca1398dbc23ba83e1795904}
      \strng{authorbibnamehash}{29c20c203ca1398dbc23ba83e1795904}
