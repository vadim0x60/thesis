        {{hash=e0093646e23599f644f65127812bbf7b}{%
           family={Kalai},
           familyi={K\bibinitperiod},
           given={Adam\bibnamedelima Tauman},
           giveni={A\bibinitperiod\bibinitdelim T\bibinitperiod}}}%
