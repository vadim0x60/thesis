    \entry{kuznetsovaImportanceStepwiseEmbeddings2023}{online}{}
      \name{author}{5}{}{%
        {{hash=1e4b085e4c25c63040f0c8118394fe58}{%
           family={Kuznetsova},
           familyi={K\bibinitperiod},
           given={Rita},
           giveni={R\bibinitperiod}}}%
        {{hash=dce27c898c878cd0d284d80724e43ba4}{%
