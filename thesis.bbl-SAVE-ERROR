      \strng{namehash}{583f72ba9d4c42f916c30f6ada1b3a76}
      \strng{fullhash}{89ccacc77a79414bb6fe737947ca08ab}
      \strng{bibnamehash}{583f72ba9d4c42f916c30f6ada1b3a76}
      \strng{authorbibnamehash}{583f72ba9d4c42f916c30f6ada1b3a76}
      \strng{authornamehash}{583f72ba9d4c42f916c30f6ada1b3a76}
