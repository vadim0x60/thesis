        {{hash=90e1dcbd1c2274c38ef42544b4d3e856}{%
           family={Guo},
           familyi={G\bibinitperiod},
           given={Junliang},
           giveni={J\bibinitperiod}}}%
        {{hash=88c58ea43b71cda8e83ddf1181a534ce}{%
           family={Tan},
           familyi={T\bibinitperiod},
           given={Xu},
           giveni={X\bibinitperiod}}}%
        {{hash=9856bacbe78d7b7b8450e13766f6bce7}{%
           family={Bian},
           familyi={B\bibinitperiod},
           given={Jiang},
           giveni={J\bibinitperiod}}}%
        {{hash=a5351b3fe2d983db4090878e29507a33}{%
