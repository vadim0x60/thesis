    \entry{allamanis2018:survey}{article}{}
      \name{author}{4}{}{%
        {{hash=e1e5eb773f262866a97eef52c0456008}{%
           family={Allamanis},
           familyi={A\bibinitperiod},
           given={Miltiadis},
           giveni={M\bibinitperiod}}}%
        {{hash=da5b7b8e6c4783fac2351cb9c4f016c4}{%
           family={Barr},
           familyi={B\bibinitperiod},
           given={Earl\bibnamedelima T.},
           giveni={E\bibinitperiod\bibinitdelim T\bibinitperiod}}}%
        {{hash=b14eb0759a69e6f4dca80cc529ce30de}{%
           family={Devanbu},
           familyi={D\bibinitperiod},
           given={Premkumar},
           giveni={P\bibinitperiod}}}%
        {{hash=fd59f6a0549332a54ac82d8840ea6ff1}{%
           family={Sutton},
           familyi={S\bibinitperiod},
           given={Charles},
           giveni={C\bibinitperiod}}}%
      }
      \strng{namehash}{7886d3262d152e8882e12f995783030f}
      \strng{fullhash}{7d9dac7f1375d51fdf148cd0479adf4b}
      \strng{bibnamehash}{7886d3262d152e8882e12f995783030f}
      \strng{authorbibnamehash}{7886d3262d152e8882e12f995783030f}
      \strng{authornamehash}{7886d3262d152e8882e12f995783030f}
      \strng{authorfullhash}{7d9dac7f1375d51fdf148cd0479adf4b}
      \field{labelalpha}{All+18}
      \field{sortinit}{A}
      \field{sortinithash}{2f401846e2029bad6b3ecc16d50031e2}
      \field{labelnamesource}{author}
      \field{labeltitlesource}{title}
      \field{abstract}{Research at the intersection of machine learning, programming languages, and software engineering has recently taken important steps in proposing learnable probabilistic models of source code that exploit the abundance of patterns of code. In this article, we survey this work. We contrast programming languages against natural languages and discuss how these similarities and differences drive the design of probabilistic models. We present a taxonomy based on the underlying design principles of each model and use it to navigate the literature. Then, we review how researchers have adapted these models to application areas and discuss cross-cutting and application-specific challenges and opportunities.}
      \field{issn}{0360-0300}
      \field{journaltitle}{ACM Computing Surveys}
      \field{month}{7}
      \field{number}{4}
      \field{title}{A {{Survey}} of {{Machine Learning}} for {{Big Code}} and {{Naturalness}}}
      \field{volume}{51}
      \field{year}{2018}
      \field{dateera}{ce}
      \field{pages}{81:1\bibrangedash 81:37}
      \range{pages}{-1}
      \verb{doi}
      \verb 10.1145/3212695
      \endverb
    \endentry
