      \strng{namehash}{36c5d614c869617b83c336a76b3f0651}
      \strng{fullhash}{36c5d614c869617b83c336a76b3f0651}
      \strng{bibnamehash}{36c5d614c869617b83c336a76b3f0651}
      \strng{authorbibnamehash}{36c5d614c869617b83c336a76b3f0651}
