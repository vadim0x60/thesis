    \entry{fairbrotherImpactFinancialIncentives2001}{article}{}
      \name{author}{5}{}{%
        {{hash=5887432be081e46b57425792799a41cc}{%
           family={Fairbrother},
           familyi={F\bibinitperiod},
           given={Gerry},
           giveni={G\bibinitperiod}}}%
