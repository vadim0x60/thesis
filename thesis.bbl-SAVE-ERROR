    \entry{xieResiDualTransformerDual2023}{online}{}
      \name{author}{9}{}{%
        {{hash=58d25384ce8c4fc277c6f5ed360416b3}{%
           family={Xie},
           familyi={X\bibinitperiod},
           given={Shufang},
