    \entry{liventsevFullyAutonomousProgramming2023}{inproceedings}{}
      \name{author}{4}{}{%
        {{hash=38041b2d992b43ae7c442d7b84d1836f}{%
           family={Liventsev},
           familyi={L\bibinitperiod},
           given={Vadim},
