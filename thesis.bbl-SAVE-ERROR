    \entry{wangCrossFormerVersatileVision2021}{online}{}
      \name{author}{7}{}{%
        {{hash=7732155c540af0f7b63a434ddb5f9c2e}{%
           family={Wang},
           familyi={W\bibinitperiod},
           given={Wenxiao},
           giveni={W\bibinitperiod}}}%
        {{hash=b21c38bda998eac3835b79668775be54}{%
           family={Yao},
           familyi={Y\bibinitperiod},
           given={Lu},
           giveni={L\bibinitperiod}}}%
        {{hash=2b01d303567b99056235e81bd74fd50e}{%
           family={Chen},
           familyi={C\bibinitperiod},
           given={Long},
