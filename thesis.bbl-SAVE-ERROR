    \entry{liangLargeScaleSurveyUsability2024}{inproceedings}{}
      \name{author}{3}{}{%
        {{hash=0d2c78748b57ef566a41dd587e0647bb}{%
           family={Liang},
           familyi={L\bibinitperiod},
           given={Jenny\bibnamedelima T.},
           giveni={J\bibinitperiod\bibinitdelim T\bibinitperiod}}}%
        {{hash=2b0dfbdd92758e2d9f2ef8be0660c115}{%
           family={Yang},
           familyi={Y\bibinitperiod},
           given={Chenyang},
           giveni={C\bibinitperiod}}}%
        {{hash=7f3139b9d8c689d2ae47ba9be98a69f0}{%
           family={Myers},
           familyi={M\bibinitperiod},
           given={Brad\bibnamedelima A.},
           giveni={B\bibinitperiod\bibinitdelim A\bibinitperiod}}}%
