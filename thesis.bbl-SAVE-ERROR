      \field{issn}{2052-4463}
      \field{issue}{1}
      \field{journaltitle}{Scientific Data}
      \field{langid}{english}
      \field{month}{1}
      \field{number}{1}
      \field{shortjournal}{Sci Data}
      \field{title}{{{MIMIC-IV}}, a Freely Accessible Electronic Health Record Dataset}
      \field{urlday}{7}
      \field{urlmonth}{6}
      \field{urlyear}{2023}
      \field{volume}{10}
      \field{year}{2023}
