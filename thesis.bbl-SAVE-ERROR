    \entry{sovianyCurriculumLearningSurvey2022}{online}{}
      \name{author}{4}{}{%
        {{hash=fe161fb81ae3982ecac5a8e70bec048c}{%
           family={Soviany},
           familyi={S\bibinitperiod},
           given={Petru},
