    \entry{staudingerXPlaneMLEnvironmentLearning2018}{article}{}
      \name{author}{3}{}{%
        {{hash=c68b783792c102ddccebf8bde01d822f}{%
           family={Staudinger},
           familyi={S\bibinitperiod},
           given={Tyler\bibnamedelima C.},
           giveni={T\bibinitperiod\bibinitdelim C\bibinitperiod}}}%
        {{hash=af4c16fed80a5861761a89f184b459f6}{%
           family={Jorgensen},
           familyi={J\bibinitperiod},
           given={Zachary\bibnamedelima D.},
           giveni={Z\bibinitperiod\bibinitdelim D\bibinitperiod}}}%
        {{hash=cd758075f42f92059996b2365472e19d}{%
           family={Margineantu},
           familyi={M\bibinitperiod},
           given={Dragos\bibnamedelima D.},
           giveni={D\bibinitperiod\bibinitdelim D\bibinitperiod}}}%
