      \strng{namehash}{c4fd65e852d78052067a4a356e6497a4}
      \strng{fullhash}{2abf5158715ac03ceb4cf8466f4e313b}
      \strng{bibnamehash}{c4fd65e852d78052067a4a356e6497a4}
