    \entry{pandaAgencyTheoryReview2017}{article}{}
      \name{author}{2}{}{%
        {{hash=b1aa84922b75344f19666af4065b8992}{%
           family={Panda},
           familyi={P\bibinitperiod},
           given={Brahmadev},
