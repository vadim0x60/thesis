    \entry{solimanMarianCGCodeGeneration2022}{article}{}
      \name{author}{3}{}{%
        {{hash=7a36f99d75b8f66849ebd04f3c70fe08}{%
           family={Soliman},
           familyi={S\bibinitperiod},
           given={Ahmed\bibnamedelima S.},
