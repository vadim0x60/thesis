    \entry{caoGALOISBoostingDeep}{article}{}
      \name{author}{8}{}{%
        {{hash=bf667ae6b95822ee3715ee931dbc712e}{%
           family={Cao},
           familyi={C\bibinitperiod},
           given={Yushi},
           giveni={Y\bibinitperiod}}}%
        {{hash=b2da3a489205efa80130dddd7b6d1db6}{%
           family={Li},
