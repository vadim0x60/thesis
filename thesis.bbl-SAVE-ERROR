      \strng{bibnamehash}{4c949d94f24ef0451a92360b5a69840c}
      \strng{authorbibnamehash}{4c949d94f24ef0451a92360b5a69840c}
      \strng{authornamehash}{4c949d94f24ef0451a92360b5a69840c}
