    \entry{bunelLeveragingGrammarReinforcement2018}{online}{}
      \name{author}{5}{}{%
        {{hash=d27d9cf80263aed4712ce3e8d841e582}{%
           family={Bunel},
           familyi={B\bibinitperiod},
           given={Rudy},
