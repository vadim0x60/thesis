      \strng{namehash}{051f035749454ef7dcc8523084edbbec}
      \strng{fullhash}{de3f4eb74221b9e130d9683fcc82ceaf}
      \strng{bibnamehash}{051f035749454ef7dcc8523084edbbec}
      \strng{authorbibnamehash}{051f035749454ef7dcc8523084edbbec}
      \strng{authornamehash}{051f035749454ef7dcc8523084edbbec}
