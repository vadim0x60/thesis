    \entry{jannerOfflineReinforcementLearning2021}{online}{}
      \name{author}{3}{}{%
        {{hash=030220ad86ae86f35b535d317d8a98bb}{%
           family={Janner},
           familyi={J\bibinitperiod},
           given={Michael},
           giveni={M\bibinitperiod}}}%
