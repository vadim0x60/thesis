      \strng{namehash}{55758a92d773afe168751e8f0e1b7227}
      \strng{fullhash}{c3d7376a574d7d564baee7d47ce95092}
      \strng{bibnamehash}{55758a92d773afe168751e8f0e1b7227}
      \strng{authorbibnamehash}{55758a92d773afe168751e8f0e1b7227}
