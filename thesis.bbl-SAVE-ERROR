      \verb /Users/vadimliventsev/Zotero/storage/PWRI2KKT/Blondel and Roulet - 2024 - The Elements of Differentiable Programming.pdf;/Users/vadimliventsev/Zotero/storage/R8FEEH4E/2403.html
      \endverb
