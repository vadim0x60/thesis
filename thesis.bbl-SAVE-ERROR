      \strng{namehash}{e52c94b890cc820dc1c2cc2db3c8f176}
      \strng{fullhash}{e52c94b890cc820dc1c2cc2db3c8f176}
      \strng{bibnamehash}{e52c94b890cc820dc1c2cc2db3c8f176}
      \strng{authorbibnamehash}{e52c94b890cc820dc1c2cc2db3c8f176}
      \strng{authornamehash}{e52c94b890cc820dc1c2cc2db3c8f176}
