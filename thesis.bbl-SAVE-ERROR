    \entry{dongSelfcollaborationCodeGeneration2023}{online}{}
      \name{author}{4}{}{%
        {{hash=56a7833adcd73ab730592b30b1fa8ee2}{%
           family={Dong},
           familyi={D\bibinitperiod},
           given={Yihong},
