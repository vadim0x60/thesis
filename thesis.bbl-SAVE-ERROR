      \strng{namehash}{5ddc21e5efd3512656cfd72da8cd2d6a}
      \strng{fullhash}{3e1e62795046c071114d6bc4b4c0be2a}
      \strng{bibnamehash}{5ddc21e5efd3512656cfd72da8cd2d6a}
      \strng{authorbibnamehash}{5ddc21e5efd3512656cfd72da8cd2d6a}
      \strng{authornamehash}{5ddc21e5efd3512656cfd72da8cd2d6a}
