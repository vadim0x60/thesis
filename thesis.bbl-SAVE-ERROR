    \entry{soPrimerSearchingEfficient2022}{online}{}
      \name{author}{6}{}{%
        {{hash=a3ac9cc34caf5dd3a38c9639c3bc9496}{%
           family={So},
           familyi={S\bibinitperiod},
           given={David\bibnamedelima R.},
