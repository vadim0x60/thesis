    \entry{leiPlanningDrivenProgrammingLarge2025}{online}{}
      \name{author}{4}{}{%
        {{hash=033f0c7c0e142c6fb8c2db039f1fee4b}{%
           family={Lei},
           familyi={L\bibinitperiod},
           given={Chao},
