      \strng{namehash}{15a621719b6edf248b5978a25ec00dbe}
      \strng{fullhash}{15a621719b6edf248b5978a25ec00dbe}
      \strng{bibnamehash}{15a621719b6edf248b5978a25ec00dbe}
      \strng{authorbibnamehash}{15a621719b6edf248b5978a25ec00dbe}
