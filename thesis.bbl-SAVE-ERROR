      \strng{namehash}{fcf4d8ede22513360603bbe534eeb7df}
      \strng{fullhash}{fcf4d8ede22513360603bbe534eeb7df}
      \strng{bibnamehash}{fcf4d8ede22513360603bbe534eeb7df}
